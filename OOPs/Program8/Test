package com.yash.Program8;

import java.time.LocalDate;

public class Test {
	
	public static void main(String[] args) {
		
		Electronics el1 = new Mobile(101, "Silver", LocalDate.of(2022, 1, 3));
		Electronics el2 = new LCD(102, "LEDs", LocalDate.of(2022, 10, 23));
		Electronics el3 = new Laptop(103, "P type", LocalDate.of(2022, 8, 12));		
		
		System.out.println("Mobile ["+el1+"]");
		System.out.println("LCD ["+el2+"]");
		System.out.println("Laptop ["+el3+"]");
		
	}
}
